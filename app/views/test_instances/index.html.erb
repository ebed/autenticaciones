<p id="notice"><%= notice %></p>

<h1>Listing Test Instances</h1>

<table>
  <thead>
    <tr>
      <th>Test</th>
      <th>Fecha</th>
      <th>Ficha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @test_instances.each do |test_instance| %>
      <tr>
        <td><%= test_instance.test_id %></td>
        <td><%= test_instance.fecha %></td>
        <td><%= test_instance.ficha_id %></td>
        <td><%= link_to 'Show', patient_ficha_registro_test_instance_path(:id => test_instance.id) %></td>
        <td><%= link_to 'Edit', edit_patient_ficha_registro_test_instance_path   %></td>
        <td><%= link_to 'Destroy', patient_ficha_registro_test_instance_path(:id => test_instance.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test instance', new_patient_ficha_registro_test_instance_path %>
