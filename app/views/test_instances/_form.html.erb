
<%= simple_form_for  ([@patient, @ficha, @registro, @test_instance])  do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :test_id,:collection => @tests, :value_method => :id,:label => "Prueba" ,:include_blank => false %>
    <%= f.input :fecha %>

  </div>


    <div class="form-actions">
      <%= f.button :submit %>
  </div>
<% end %>
